<Window x:Class="CinemaReviewWPF.SignInPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CinemaReviewWPF"
        mc:Ignorable="d"
        Title="SignInPage" Height="450" Width="800">
    <Grid RenderTransformOrigin="1.327,0.84">
        <TextBox HorizontalAlignment="Left" Height="22" Margin="327,174,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="174" TextChanged="TextBox_TextChanged_2"/>
        <Label Content="Enter Password:" HorizontalAlignment="Left" Margin="327,218,0,0" VerticalAlignment="Top" Height="27" Width="95"/>
        <PasswordBox HorizontalAlignment="Left" Margin="327,250,0,0" VerticalAlignment="Top" Width="174" Height="22"/>
        <Button Content="Login" HorizontalAlignment="Left" Margin="329,287,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click"/>
        <Button Content="Register" HorizontalAlignment="Left" Margin="412,287,0,0" VerticalAlignment="Top" Width="89" Click="Button_Click_1"/>
        <Label Content="Enter Login:" HorizontalAlignment="Left" Margin="329,142,0,0" VerticalAlignment="Top" Height="27" Width="95"/>
    </Grid>
</Window>
